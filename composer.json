{
    "name": "laut/twig-lumen",
    "description": "Adds the power of Twig to Lumen",
    "keywords": ["Lumen", "Twig"],
    "homepage": "https://github.com/lauthiamkok/twig-lumen",
    "license": "MIT",
    "authors": [
        {
            "name": "Lau Thiam Kok",
            "email": "lau.thiamkok@yahoo.co.uk",
            "homepage": "http://lauthiamkok.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "twig/twig": "~1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "LumenTwig\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
          "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
